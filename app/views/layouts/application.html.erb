<!DOCTYPE html>
<html>
  <head>
    <title>FoodKick</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="font-awesome-animation.min.css">
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Lobster">


  </head>
  <body>
      <%= render 'shared/navbar' %>
      <div id="wrapper">
        <%= render 'shared/flashes' %>
        <%= yield %>
        <%= render 'shared/footer' if @footer %>
      </div>
    <%= javascript_include_tag "https://maps.google.com/maps/api/js?sensor=false&key=AIzaSyAT-JeQCCfJD4kEFXeB5Ku01nZwP9cR5p8 " %>

    <%= javascript_include_tag "https://google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js" %>
    <%= javascript_include_tag 'application' %>
    <%= yield(:after_js) %>
  </body>
</html>




